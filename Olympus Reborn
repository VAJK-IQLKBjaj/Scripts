local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Hades God Hub | Olympus Reborn", "BloodTheme")
local Tab = Window:NewTab("Characters")
local Section = Tab:NewSection("Change character")

Section:NewButton("Hades", "Play as Hades", function()
    local args = {
        [1] = "Hades"
    }

    game:GetService("ReplicatedStorage").PickCharacter:FireServer(unpack(args))
end)

