    -- This script was generated by Hydroxide's RemoteSpy: https://github.com/Upbolt/Hydroxide

    local ohString1 = "rpunch2"
    local ohVector32 = Vector3.new(667.02294921875, 30.049619674682617, -174.54847717285156)

    game:GetService("ReplicatedStorage").Witches.GoodWitches.Prue.PowerEvents.PrimroseEvents:FireServer(ohString1,
        ohVector32)

end)

